    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run ClientApp:serve-ssr",
    "serve:ssr": "node dist/ClientApp/server/main.js",
    "build:ssr:development": "ng build && ng run ClientApp:server:development",
    "build:ssr:production": "npm run tailwind:build && ng build --configuration production && ng run ClientApp:server:production",
    "prerender": "ng run ClientApp:prerender",
    "analyze-bundle": "ng build --configuration=analyze-bundle && source-map-explorer dist/**/*.js --no-border-checks",
